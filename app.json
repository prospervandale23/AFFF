{
  "expo": {
    "name": "Fishing Buddy",
    "slug": "fishing-buddy", 
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fishingbuddy",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pvandale766.fishingbuddy",
      "buildNumber": "1",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Fishing Buddy uses your location to find nearby fishing buddies and show local fishing conditions.",
        "NSLocationAlwaysUsageDescription": "Fishing Buddy uses your location to notify you when fishing buddies are nearby.",
        "NSCameraUsageDescription": "Take photos of your catches to share with the community.",
        "NSPhotoLibraryUsageDescription": "Choose photos from your library to share your fishing adventures.",
        "NSPhotoLibraryAddUsageDescription": "Save catch photos to your photo library."
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0B1220"
      },
      "package": "com.pvandale766.fishingbuddy",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_MEDIA_IMAGES"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0B1220"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Fishing Buddy to use your location to find nearby fishing buddies."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/5936630a-1186-46dd-a900-09076b302acf"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5936630a-1186-46dd-a900-09076b302acf"
      }
    },
    "owner": "pvandale766",
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}