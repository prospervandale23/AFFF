{
  "expo": {
    "name": "Catch Connect",
    "slug": "catch-connect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "catchconnect",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pvandale766.catchconnect",
      "buildNumber": "1",
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Catch Connect uses your location to find nearby fishing buddies and show local fishing conditions.",
        "NSCameraUsageDescription": "Take photos of your catches and profile to share with the fishing community.",
        "NSPhotoLibraryUsageDescription": "Choose photos from your library to upload to your profile and share your fishing adventures.",
        "NSPhotoLibraryAddUsageDescription": "Save catch photos to your photo library."
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0B1220"
      },
      "package": "com.pvandale766.catchconnect",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_MEDIA_IMAGES",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0B1220"
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Allow Catch Connect to use your location to find nearby fishing buddies."
        }
      ],
      "expo-image-picker",
      "expo-apple-authentication",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "owner": "pvandale766",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6574fa28-f725-4813-8047-4ba86ec1be9b"
      }
    }
  }
}